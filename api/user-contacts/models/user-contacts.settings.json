{
  "connection": "default",
  "collectionName": "user_contacts",
  "info": {
    "name": "UserContacts"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "text": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "phone": {
      "type": "string"
    },
    "linkedin": {
      "type": "string"
    },
    "Picture": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    }
  }
}
